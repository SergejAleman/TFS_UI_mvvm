<UserControl x:Class="TFS_UI_mvvm.Views.UserControls.UserControlA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TFS_UI_mvvm.Views.UserControls"
             mc:Ignorable="d" 
             xmlns:vm="clr-namespace:TFS_UI_mvvm.ViewModels.UserControlViewModels"
             d:DataContext="{d:DesignInstance vm:UserControlA_ViewModel, IsDesignTimeCreatable=True}"
             Height="200" Width="300">
    
    <Grid Background="LightBlue">
        <TextBlock Text="{Binding Title}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16"/>
    </Grid>
</UserControl>
